		"stylelint-config-recommended-scss": "13.1.0",
		"stylelint-prettier": "4.0.2",
		"stylelint-scss": "5.3.0"
	},
	"scripts": {
		"build": "sass --style=compressed --load-path=node_modules/normalize-scss/sass/ assets/scss:.",
